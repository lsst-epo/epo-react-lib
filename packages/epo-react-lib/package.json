{
  "name": "@rubin-epo/epo-react-lib",
  "version": "1.0.5",
  "author": "Rubin EPO",
  "license": "MIT",
  "homepage": "https://lsst-epo.github.io/epo-react-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/epo-react-lib"
  },
  "type": "module",
  "supportedLocales": [
    "en",
    "es"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:clean": "clean-publish",
    "storybook": "yarn run test:generate-output && start-storybook -p 6006",
    "deploy-storybook": "STORYBOOK_PROD=true && storybook-to-ghpages",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "main": "./dist/epo-lib.umd.js",
  "module": "./dist/epo-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/epo-lib.es.js",
      "require": "./dist/epo-lib.umd.js"
    }
  },
  "dependencies": {
    "flickity": "^3.0.0",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",

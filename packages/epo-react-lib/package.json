{
  "name": "@rubin-epo/epo-react-lib",
  "description": "Rubin Observatory Education & Public Outreach team React UI library.",
  "version": "1.2.9",
  "author": "Rubin EPO",
  "license": "MIT",
  "homepage": "https://lsst-epo.github.io/epo-react-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/epo-react-lib"
  },
  "supportedLocales": [
    "en",
    "es"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "publish:clean": "yarn build && clean-publish",
    "storybook": "yarn run test:generate-output && start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "deploy-storybook": "storybook-to-ghpages",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true"
  },
  "files": [
    "CHANGELOG.md",
    "README.md",
    "dist"
  ],
  "main": "./dist/epo-lib.umd.js",
  "module": "./dist/epo-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/epo-lib.es.js",
      "require": "./dist/epo-lib.umd.js"
    }
  },
  "dependencies": {
    "flickity": "^3.0.0",
    "focus-trap": "^7.4.2",
    "react-player": "^2.12.0",
    "react-share": "^4.4.1",
    "react-slider": "^2.0.4",
    "react-uid": "^2.3.2",
    "styled-components": "^5.3.6"
  },
  "devDependencies": {
    "@babel/core": "^7.19.1",
    "@babel/preset-env": "^7.19.1",
    "@babel/preset-react": "^7.18.6",
    "@babel/preset-typescript": "^7.18.6",
    "@castiron/style-mixins": "^1.0.6",
    "@headlessui/react": "^1.7.5",
    "@storybook/addon-actions": "^6.5.12",
    "@storybook/addon-essentials": "^6.5.12",
    "@storybook/addon-interactions": "^6.5.12",
    "@storybook/addon-jest": "^6.5.12",
    "@storybook/addon-links": "^6.5.12",
    "@storybook/builder-vite": "0.4.2",
    "@storybook/cli": "^6.5.12",
    "@storybook/react": "^6.5.12",
    "@storybook/storybook-deployer": "^2.8.16",
    "@storybook/testing-library": "^0.0.13",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/flickity": "^2.2.7",
    "@types/jest": "^29.0.3",
    "@types/react": "^18.0.17",
    "@types/react-dom": "^18.0.6",
    "@types/react-slider": "^1.3.1",
    "@types/styled-components": "^5.1.26",
    "@vitejs/plugin-react": "^4.0.0",
    "babel-loader": "^8.2.5",
    "clean-publish": "^4.0.1",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "jest": "^29.0.3",
    "jest-canvas-mock": "^2.4.0",
    "jest-environment-jsdom": "^29.0.3",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0",
    "storybook-react-i18next": "^1.1.2",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
    "typescript": "^4.6.4",
    "vite": "^4.3.3",
    "vite-plugin-dts": "^2.3.0"
  },
  "peerDependencies": {
    "i18next": "^22.4.10",
    "next": "^13.0.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0"
  },

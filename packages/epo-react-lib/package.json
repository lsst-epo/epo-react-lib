{
  "name": "@rubin-epo/epo-react-lib",
  "version": "1.0.2",
  "author": "Rubin EPO",
  "license": "MIT",
  "homepage": "https://lsst-epo.github.io/epo-react-lib",
  "repository": {
    "type": "git",
    "url": "https://github.com/lsst-epo/epo-react-lib"
  },
  "type": "module",
  "supportedLocales": [
    "en",
    "es"
  ],
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "prepack": "cd ../.. && cp README.md packages/epo-react-lib && cd packages/epo-react-lib && json -f package.json -I -e \"delete this.devDependencies; delete this.scripts\"",
    "storybook": "yarn run test:generate-output && start-storybook -p 6006",
    "deploy-storybook": "STORYBOOK_PROD=true && storybook-to-ghpages",
    "test": "jest",
    "test:generate-output": "jest --json --outputFile=.jest-test-results.json || true"
  },
  "files": [
    "README.md",
    "dist"
  ],
  "main": "./dist/epo-lib.umd.js",
  "module": "./dist/epo-lib.es.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/epo-lib.es.js",
      "require": "./dist/epo-lib.umd.js"
    }
  },
  "dependencies": {
    "flickity": "^3.0.0",
    "i18next": "^22.0.4",
    "i18next-browser-languagedetector": "^7.0.1",
    "i18next-http-backend": "^2.0.1",
    "react-i18next": "^12.0.0",
    "react-player": "^2.11.0",
    "react-uid": "^2.3.2",
    "styled-components": "^5.3.5"
  },
  "peerDependencies": {
    "i18next": "^22.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-i18next": "^12.0.0"
  }
}

    "jest-environment-jsdom": "^29.0.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "storybook-react-i18next": "^1.1.2",
    "ts-jest": "^29.0.2",
    "ts-node": "^10.9.1",
